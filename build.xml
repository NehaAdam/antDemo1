
<project default="run" name="myAntprj">
	<target name="run" depends="compile">
		<java classname="com.psl.demo.Demo1">
			<classpath path="staging"/>
		</java>
	
	</target>
	<target name="compile">
		<javac includeantruntime="false" srcdir="src/com/psl/demo"/>
	</target>
	<target name="compress">
		<jar jarfile="antDemo1.jar" basedir="bin/com/psl/demo" includes="*.class"></jar>
	</target>
</project>